<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.EmployeeAdmin">

    <uses-permission android:name="android.permission.INTERNET" /> <!-- <uses-permission android:name=""/> -->
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.EmployeeAdmin.notice.DeleteNotice"></activity>
        <activity android:name="com.example.EmployeeAdmin.faculty.UpdateTeacher" />
        <activity android:name="com.example.EmployeeAdmin.faculty.addTeacher" />
        <activity android:name="com.example.EmployeeAdmin.faculty.UpdateFaculty" />
        <activity android:name="com.example.EmployeeAdmin.UploadPDF" />
        <activity android:name="com.example.EmployeeAdmin.UploadImage" />
        <activity android:name="com.example.EmployeeAdmin.notice.AddNotice" />
        <activity android:name="com.example.EmployeeAdmin.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>